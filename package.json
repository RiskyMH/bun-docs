{
  "name": "fumadocs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "bun pull-docs && next build",
    "dev": "next dev --turbo",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "pull-docs": "bun scripts/pull-docs.ts --download-only",
    "sync-docs": "bun scripts/pull-docs.ts && bun migrate-docs.ts && git apply manual-fixes.patch --reject",
    "sync-docs:save": "bun scripts/pull-docs.ts && bun migrate-docs.ts && git diff -R content/ > manual-fixes.patch && git restore content/",
    "check-links": "bun scripts/check-links.ts"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.28",
    "@ai-sdk/react": "^2.0.109",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-presence": "^1.1.5",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@types/bun": "^1.3.3",
    "ai": "^5.0.108",
    "babel-plugin-react-compiler": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "fumadocs-core": "16.2.3",
    "fumadocs-mdx": "14.1.0",
    "fumadocs-ui": "16.2.3",
    "gatsby-remark-find-replace": "^0.3.0",
    "geist": "^1.5.1",
    "hast": "^1.0.0",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "lucide-react": "^0.556.0",
    "next": "^16.1.0-canary.15",
    "next-themes": "^0.4.6",
    "next-validate-link": "^1.6.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-tweet": "^3.2.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.2",
    "tailwind-merge": "^3.4.0",
    "unist-util-visit": "^5.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "@types/mdx": "^2.0.13",
    "@tailwindcss/postcss": "^4.1.17",
    "tailwindcss": "^4.1.17",
    "postcss": "^8.5.6"
  }
}